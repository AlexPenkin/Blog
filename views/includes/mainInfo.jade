block mainInfo
  doctype html
  html(lang="ru")
    head
      style.
        @font-face {
          font-family: 'San Francisco Display Semibold';
            src: url('css/fonts/SanFranciscoDisplay-Semibold.eot');
            src: url('css/fonts/SanFranciscoDisplay-Semibold.eot?#iefix') format('embedded-opentype'),
              url('css/fonts/SanFranciscoDisplay-Semibold.woff') format('woff'),
              url('css/fonts/SanFranciscoDisplay-Semibold.ttf') format('truetype');
            font-weight: 600;
            font-style: normal;
        }

        @font-face {
          font-family: 'San Francisco Display Ultralight';
          src: url('css/fonts/SanFranciscoDisplay-Ultralight.eot');
          src: url('css/fonts/SanFranciscoDisplay-Ultralight.eot?#iefix') format('embedded-opentype'),
            url('css/fonts/SanFranciscoDisplay-Ultralight.woff') format('woff'),
            url('css/fonts/SanFranciscoDisplay-Ultralight.ttf') format('truetype');
          font-weight: 100;
          font-style: normal;
        }

        @font-face {
          font-family: 'San Francisco Display Regular';
          src: url('css/fonts/SanFranciscoDisplay-Regular.eot');
          src: url('css/fonts/SanFranciscoDisplay-Regular.eot?#iefix') format('embedded-opentype'),
